//for

for(let index=0;index<10;index++){
    const element =index;
    if(element==5){
        console.log("5 is best number")
    }
    console.log(element);
}
//console.log(element);

for(let i=1;i<=10;i++){
    console.log(`Outer loop value ${i}`);
    for(let j=1;j<10;j++){
        console.log(`Inner loop value ${j} and ${i}`);
        console.log(i+'*'+j+ '='+i*j);
    }
}

let array =["Mango","apple","banana"];
console.log(array.length);
for(let index=0;index<array.length;index++){
    const element =array[index];
        console.log(element);
}


for(let i=1;i<=10;i++){
        if(i==5){
            console.log("5 detected");
            break;
        }
        console.log(i);
    }

    for(let i=1;i<=10;i++){
        if(i==5){
            console.log("5 detected");
            continue;
        }
        console.log(i);
    }
